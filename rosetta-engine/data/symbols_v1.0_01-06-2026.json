        "recipes": { "folioRange": "f103r-f116v", "primaryDPositions": [4], "theme": "Methodology" }
      }
    },
    "indus": {
      "id": "indus",
      "name": "Indus Valley Script",
      "period": "2600-1900 BCE",
      "location": "Over 4,000 inscribed objects across Pakistan/India",
      "corpusSize": "~400-450 distinct signs",
      "writingSystem": "Possibly logo-syllabic",
      "status": "Undeciphered",
      "evidenceQuality": "High",
      "description": "The Indus Valley civilization produced seals and inscriptions showing geometric patterns that map to D-positions."
    },
    "linearA": {
      "id": "linearA",
      "name": "Linear A",
      "period": "1800-1450 BCE",
      "location": "Crete and Aegean islands",
      "corpusSize": "~7,000 signs in ~1,500 inscriptions",
      "writingSystem": "Syllabic (related to Linear B)",
      "status": "Undeciphered (language unknown)",
      "evidenceQuality": "Medium",
      "description": "Minoan script showing connections to both Indus and later Greek systems."
    },
    "rongorongo": {
      "id": "rongorongo",
      "name": "Rongorongo",
      "period": "pre-1860s CE",
      "location": "Easter Island (Rapa Nui)",
      "corpusSize": "~14,000 glyphs across 24 surviving tablets",
      "writingSystem": "Logo-syllabic (probable)",
      "status": "Undeciphered",
      "evidenceQuality": "Medium-Low",
      "description": "The script encodes meaning through the Q² transformation - the 180° flip between lines. RONGORONGO = D₃ = Healer.",
      "uniqueFeature": "Reverse Boustrophedon - every other line is read upside-down (Q² = -1 transformation)"
    },
    "phaistos": {
      "id": "phaistos",
      "name": "Phaistos Disc",
      "period": "1700 BCE?",
      "location": "Heraklion Archaeological Museum, Crete",
      "corpusSize": "241 tokens, 45 distinct signs",
      "writingSystem": "Stamped (unique manufacturing method)",
      "status": "Undeciphered",
      "evidenceQuality": "Low-Medium",
      "description": "Spiral arrangement embeds the triaxial system: Side A (31 groups) reads outward (S+ expansion), Side B (30 groups) reads inward (S- contraction)."
    }
  },
