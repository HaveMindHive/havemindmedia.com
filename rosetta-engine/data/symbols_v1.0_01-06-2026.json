    "voynich": {
      "id": "voynich",
      "name": "Voynich Manuscript",
      "period": "15th century CE?",
      "location": "Beinecke Rare Book Library, Yale University",
      "corpusSize": "~170,000 glyphs across 240 pages",
      "writingSystem": "Unique 'Voynichese' with ~30 distinct characters",
      "status": "Undeciphered",
      "evidenceQuality": "Medium-High",
      "description": "The Voynich Manuscript is a visual cryptography system where text provides extraction parameters and images provide the data field.",
